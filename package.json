{
  "name": "marked-custom-heading-id",
  "version": "2.0.13",
  "description": "marked extension template",
  "main": "./lib/index.cjs",
  "module": "./src/index.js",
  "browser": "./lib/index.umd.js",
  "types": "./src/index.d.mts",
  "type": "module",
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "keywords": [
    "marked",
    "extension",
    "custom",
    "heading",
    "id"
  ],
  "scripts": {
    "test": "jest --verbose",
    "test:cover": "jest --coverage",
    "test:types": "tsd -t src/index.d.ts -f spec/index.test-d.ts",
    "test:all": "npm run test:cover && npm run test:types && npm run lint && npm run build",
    "lint": "eslint .",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markedjs/marked-custom-heading-id.git"
  },
  "author": "Tony Brix <Tony@Brix.ninja> (https://Tony.Brix.ninja)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markedjs/marked-custom-heading-id/issues"
  },
  "homepage": "https://github.com/markedjs/marked-custom-heading-id#readme",
  "peerDependencies": {
    "marked": ">=1 <16"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@markedjs/eslint-config": "^1.0.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "jest-cli": "^29.7.0",
    "marked": "^15.0.7",
    "rollup": "^4.37.0",
    "semantic-release": "^24.2.3",
    "tsd": "^0.31.2"
  },
  "tsd": {
    "compilerOptions": {
      "paths": {
        "marked-custom-heading-id": [
          "./src"
        ]
      }
    }
  }
}
